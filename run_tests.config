<?xml version="1.0"?>
<Tests>
	<!--
		Tag  :    [BeginZone]
		Aim  :    Study starting zone number effect
	    Trace:    sequential_write_[4KB..64MB]_from_zone[1,17,..,113]_1GB
		Page Allocation: 
	-->
	<Suite name="BeginZone" desc="Examine difference of beginning zone number~1"
	       run="false">
		<Test desc="Zone number starts from 1">
			<SSDConfig>ssdconfig_zone.xml</SSDConfig>
			<Workload>workload/BeginZone/workload_zone_beginning_zone_1.xml</Workload>
		</Test>
		<Test desc="Zone number starts from 2">
			<SSDConfig>ssdconfig_zone.xml</SSDConfig>
			<Workload>workload/BeginZone/workload_zone_beginning_zone_2.xml</Workload>
		</Test>
		<Test desc="Zone number starts from 3">
			<SSDConfig>ssdconfig_zone.xml</SSDConfig>
			<Workload>workload/BeginZone/workload_zone_beginning_zone_3.xml</Workload>
		</Test>
		<Test desc="Zone number starts from 4">
			<SSDConfig>ssdconfig_zone.xml</SSDConfig>
			<Workload>workload/BeginZone/workload_zone_beginning_zone_4.xml</Workload>
		</Test>
		<Test desc="Zone number starts from 5">
			<SSDConfig>ssdconfig_zone.xml</SSDConfig>
			<Workload>workload/BeginZone/workload_zone_beginning_zone_5.xml</Workload>
		</Test>
		<Test desc="Zone number starts from 6">
			<SSDConfig>ssdconfig_zone.xml</SSDConfig>
			<Workload>workload/BeginZone/workload_zone_beginning_zone_6.xml</Workload>
		</Test>
		<Test desc="Zone number starts from 7">
			<SSDConfig>ssdconfig_zone.xml</SSDConfig>
			<Workload>workload/BeginZone/workload_zone_beginning_zone_7.xml</Workload>
		</Test>
		<Test desc="Zone number starts from 8">
			<SSDConfig>ssdconfig_zone.xml</SSDConfig>
			<Workload>workload/BeginZone/workload_zone_beginning_zone_8.xml</Workload>
		</Test>
	</Suite>
	
	<!--
		Tag  :    [IndHW]
		Aim  :    Study Channel, Chip, Die, Plane write effects 
	    Trace:    sequential_write_[4KB..16MB]_from_zone1_4GB
	-->
	<Suite name="IndHW" desc="Study Channel, Chip, Die, Plane write effects"
		   run="false">
		<Test desc="Minimum pallelism as baseline 1111">
			<SSDConfig>ssdconfig_zone.xml</SSDConfig>
			<Workload>workload/IndHW/workload_zone_min_effect.xml</Workload>
		</Test>
		<Test desc="Maximum pallelism as baseline 4222">
			<SSDConfig>ssdconfig_zone.xml</SSDConfig>
			<Workload>workload/IndHW/workload_zone_max_effect.xml</Workload>
		</Test>
		<Test desc="Examine channel parallelism effect 4111">
			<SSDConfig>ssdconfig_zone.xml</SSDConfig>
			<Workload>workload/IndHW/workload_zone_channel_effect.xml</Workload>
		</Test>
		<Test desc="Examine chip parallelism effect 1211">
			<SSDConfig>ssdconfig_zone.xml</SSDConfig>
			<Workload>workload/IndHW/workload_zone_chip_effect.xml</Workload>
		</Test>
		<Test desc="Examine die parallelism effect 1121">
			<SSDConfig>ssdconfig_zone.xml</SSDConfig>
			<Workload>workload/IndHW/workload_zone_die_effect.xml</Workload>
		</Test>
		<Test desc="Examine plane parallelism effect 1112">
			<SSDConfig>ssdconfig_zone.xml</SSDConfig>
			<Workload>workload/IndHW/workload_zone_plane_effect.xml</Workload>
		</Test>
	</Suite>
	<!--
		Tag  :    [ZoneSize]
