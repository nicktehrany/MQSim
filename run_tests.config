<?xml version="1.0"?>
<Tests>
    <!--  -->
	<!--
		Tag  :    [BeginZone]
		Aim  :    Study starting zone number effect
	    Trace:    sequential_write_[4KB..64MB]_from_zone[1,17,..,113]_4GB
		Page Allocation: 
	-->
	<Suite tag="BeginZone" desc="Examine difference of beginning zone number~1"
	       run="true">
		<Test desc="Zone number starts from 1">
			<UID>98</UID>
			<SSDConfig>ssdconfig_zone.xml</SSDConfig>
			<Workload>workload/BeginZone/workload_zone_beginning_zone_1.xml</Workload>
		</Test>
		<Test desc="Zone number starts from 2">
			<UID>99</UID>
			<SSDConfig>ssdconfig_zone.xml</SSDConfig>
			<Workload>workload/BeginZone/workload_zone_beginning_zone_17.xml</Workload>
		</Test>
		<Test desc="Zone number starts from 24">
			<UID>100</UID>
			<SSDConfig>ssdconfig_zone.xml</SSDConfig>
			<Workload>workload/BeginZone/workload_zone_beginning_zone_33.xml</Workload>
		</Test>
		<Test desc="Zone number starts from 50">
			<UID>101</UID>
			<SSDConfig>ssdconfig_zone.xml</SSDConfig>
			<Workload>workload/BeginZone/workload_zone_beginning_zone_49.xml</Workload>
		</Test>
	</Suite>
	
	<!--
		Tag  :    [IndHW]
		Aim  :    Study Channel, Chip, Die, Plane write effects 
	    Trace:    sequential_write_8KB_from_zone2_1GB
	-->
	<Suite tag="IndHW" desc="Study Channel, Chip, Die, Plane write effects"
		   run="false">
		<Test desc="Minimum pallelism as baseline 1111">
			<UID>102</UID>
			<SSDConfig>ssdconfig_zone.xml</SSDConfig>
			<Workload>workload/IndHW/workload_zone_min_effect.xml</Workload>
		</Test>
		<Test desc="Examine channel parallelism effect 4111">
			<UID>103</UID>
			<SSDConfig>ssdconfig_zone.xml</SSDConfig>
			<Workload>workload/IndHW/workload_zone_channel_effect.xml</Workload>
		</Test>
		<Test desc="Examine chip parallelism effect 1211">
			<UID>104</UID>
			<SSDConfig>ssdconfig_zone.xml</SSDConfig>
			<Workload>workload/IndHW/workload_zone_chip_effect.xml</Workload>
		</Test>
		<Test desc="Examine die parallelism effect 1121">
			<UID>105</UID>
			<SSDConfig>ssdconfig_zone.xml</SSDConfig>
			<Workload>workload/IndHW/workload_zone_die_effect.xml</Workload>
