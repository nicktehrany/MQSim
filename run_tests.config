		</Test>
		<Test desc="256MB">
			<SSDConfig>workload/ZoneSize/ssdconfig/ssdconfig_zone_256.xml</SSDConfig>
			<Workload>workload/ZoneSize/workload_zone_max_256.xml</Workload>
		</Test>
		<Test desc="512MB">
			<SSDConfig>workload/ZoneSize/ssdconfig/ssdconfig_zone_512.xml</SSDConfig>
			<Workload>workload/ZoneSize/workload_zone_max_512.xml</Workload>
